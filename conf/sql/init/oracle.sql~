/*set the schema if there is one*/
/*eval*/ ALTER SESSION SET CURRENT_SCHEMA=$db.default.schema$;


CREATE TABLE PLAY_EVOLUTIONS
(   ID NUMBER NOT NULL ENABLE, 
    HASH VARCHAR2(255 BYTE) NOT NULL ENABLE, 
    APPLIED_AT TIMESTAMP (6) NOT NULL ENABLE, 
    APPLY_SCRIPT CLOB, 
    REVERT_SCRIPT CLOB, 
    STATE VARCHAR2(255 BYTE), 
    LAST_PROBLEM CLOB, 
    CONSTRAINT "PLAY_EVOLUTIONS_PK" PRIMARY KEY ("ID")
)
